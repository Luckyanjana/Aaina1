
@{
    ViewData["Title"] = "Index";

}

<link href="~/plugins/fullcalendar/main.css" rel="stylesheet" />
<style>
    .fc-license-message {
    display:none!important;
    }
</style>
<div class="top-Rbox1">
    <div class="top-Ltext">
        <div class="overlap"></div>
        <a href="javascript:void(0);" class="naleftbtn"><span></span></a>
        <div class="top-Ltextli">
            <ul>
                <li>Game</li>
                <li>/</li>
                <li>Schedule</li>
            </ul>
        </div>
    </div>
</div>

<div class="middlebox">
    <div class="tablediv">
        <div class="row">
            <div class="col-md-3">
                <input type="text" id="txt_date" class="form-control" />
                <input type="hidden" id="api_url" value="/admin/sessionschedule/getevent" />
                <input type="hidden" id="api_agenda_url" value="/admin/sessionschedule/details" />
                
            </div>
            <div class="col-md-3">
                <select id="game_id" class="form-control" asp-items="@(new SelectList(ViewBag.GameList,"Id","Name"))">
                    <option value="">Select</option>
                </select>
            </div>

        </div>
        <div id="calendar"></div>
    </div>
</div>




@section scripts{
    <script src="~/plugins//fullcalendar/main.js"></script>
    <script src="~/js/Customejs/SessionSchedule/Event.js" asp-append-version="true"></script>

}